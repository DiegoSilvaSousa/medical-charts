<div class="container">
  <h2>Database</h2>
</div>

<div class="row">
  <div class="col s10 offset-s1">
    <div class="col s4">
      <div class="col s12 m12">
        <%= link_to admin_basic_treatments_url do %>
          <div class="card-panel green waves-effect waves-light max-width">
            <h4 class="white-text flow-text">Tratamentos Básicos
              <i class="mdi-action-grade right"></i>
            </h4>
          </div>
        <% end -%>
      </div>
    </div>

    <div class="col s4">
      <div class="col s12 m12">
        <%= link_to admin_basic_questions_url do %>
          <div class="card-panel pink waves-effect waves-light max-width">
            <h4 class="white-text flow-text">Questionário Histórico
              <i class="mdi-action-history right"></i>
            </h4>
          </div>
        <% end -%>
      </div>
    </div>
  </div>
</div>
